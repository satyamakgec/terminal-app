{
  "name": "tech-docs-api-server",
  "version": "0.1.0",
  "description": "API server for the terminal app docs",
  "author": "Terminal Inc.",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "deploy": "sh ./deploy.sh",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satyamakgec/tech-docs-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API"
  ],
  "dependencies": {
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^6.0.0",
    "express": "^4.15.2",
    "express-rate-limit": "^3.0.0",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^14.0.0",
    "lodash": "^4.17.4",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "mongoose": "^5.0.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "pm2": "^3.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^1.0.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.11.0",
    "nyc": "^12.0.1",
    "opn-cli": "^3.1.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  }
}
